<% if @categories_created %>
    <% @categories_created.each do |category| %>
        <p>
        created
        <%= category %>
 
    <% end %>
<% end %>
<% if @categories_skipped %>
    <% @categories_skipped.each do |category| %>
        <p>
        skipped
        <%= category %>
        (すでに存在するnumberが指定されたため)

    <% end %>
<% end %>
<% if @categories_deleted %>
    <% @categories_deleted.each do |category| %>
        <p>
        deleted 
        a category

    <% end %>
<% end %>
<% if @categories_kept %>
    <% @categories_kept.each do |category| %>
        <p>
        kept
        <%= category %>
        （クッキーにないので、多分再読み込みとかされて意図しない動きしてそうなので念の為消さないでおく）
    <% end %>
<% end %>
